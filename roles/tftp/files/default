default vesamenu.c32
prompt 1
timeout 600

display boot.msg

label RHEL8
  menu label ^Install RHEL 8 Beta
  menu default
  kernel images/RHEL8/vmlinuz
  append initrd=images/RHEL8/initrd.img ip=dhcp inst.repo=https://downloads.redhat.com/redhat/rhel/rhel-8-beta/baseos/x86_64/os/
label F30Server
  menu label Install ^Fedora 30 Server
  kernel images/F30Server/vmlinuz
  append initrd=images/F30Server/initrd.img ip=dhcp inst.repo=https://mirror.steadfastnet.com/fedora/development/30/Server/x86_64/os/
label F30SilverBlue
  menu label ^Fedora 30 Silverblue
  kernel images/F30Silver/vmlinuz
  append initrd=images/F30Server/initrd.img ip=dhcp inst.repo=https://mirror.steadfastnet.com/fedora/development/30/Silverblue/x86_64/os/
label F30WorkStation
  menu label Install ^Fedora 30 WorkStation
  kernel images/F30WkStn/vmlinuz
  append initrd=images/F30WkStn/initrd.img ip=dhcp https://mirror.steadfastnet.com/fedora/development/30/Workstation/x86_64/os/
label CentOS7
  menu label ^CentOS 7
  kernel images/CentOS7/vmlinuz
  append initrd=images/CentOS7/initrd.img ip=dhcp https://mirror.steadfastnet.com/centos/7/os/x86_64/
label local
  menu label Boot from ^local drive
  localboot 0xffff
